<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ictti.omas.business.entity">
	<class
		name="Specialize"
		table="specialize"
		lazy="false"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="spid"
		>
			<generator class="identity"/>
		</id>

		<property
			name="specializetype"
			column="specializetype"
			type="string"
			not-null="true"
			length="25"
		/>
	

		<set name="hosSpecs" inverse="true" lazy="false">
			<key column="spid"/>
			<one-to-many class="HosSpec"/>
		</set>

		<set name="doctors" inverse="true" lazy="false">
			<key column="spid"/>
			<one-to-many class="Doctor"/>
		</set>


	</class>	
</hibernate-mapping>