<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ictti.omas.business.entity">
	<class
		name="PatientappointmentlistView"
		table="patientappointmentlist_view"
	>
		<meta attribute="sync-DAO">false</meta>
		<id
			name="id"
			type="integer"
			column="pid"
		>
			<generator class="identity"/>
		</id>

		<property
			name="pregdate"
			column="pregdate"
			type="date"
			not-null="true"
			length="10"
		/>
	
	<property
			name="appointmentdt"
			column="appointmentdt"
			type="date"
			not-null="false"
			length="10"
		/>


	</class>	
</hibernate-mapping>